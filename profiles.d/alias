alias cls=clear
alias vim=nvim
alias vi=nvim
alias v=nvim
alias ll="ls -alsHh"
alias cat=ccat
alias top=htop

alias proxylup='export http_proxy=127.0.0.1:8001;export https_proxy=$http_proxy'
alias proxyrup='export http_proxy=192.168.1.3:8890;export https_proxy=$http_proxy'

alias proxydown='unset http_proxy;unset https_proxy'

alias remoteAdbDebug='adb shell setprop service.adb.tcp.port 5555'

# Dir alias

alias ggr="cd ~/workspace/go/src/ && ls -als"
alias gws="cd ~/workspace && ls -als"

